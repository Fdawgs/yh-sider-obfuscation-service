{
	"name": "ydh-sider-obfuscation-service",
	"version": "9.1.0",
	"description": "Yeovil District Hospital NHSFT's SIDeR Contextual Link Obfuscation Service",
	"main": "src/app.js",
	"repository": "git+https://github.com/Fdawgs/ydh-sider-obfuscation-service.git",
	"homepage": "https://github.com/Fdawgs/ydh-sider-obfuscation-service",
	"bugs": {
		"url": "https://github.com/Fdawgs/ydh-sider-obfuscation-service/issues"
	},
	"license": "MIT",
	"author": "Frazer Smith <frazer.smith@ydh.nhs.uk>",
	"engines": {
		"node": ">=14.0.0"
	},
	"scripts": {
		"benchmark": "autocannon -a 1000 \"http://0.0.0.0:8204/redirect?patient=https://fhir.nhs.uk/Id/nhs-number|9449304513&birthdate=1934-10-23&location=https://fhir.nhs.uk/Id/ods-organization-code|RA4&practitioner=https://sider.nhs.uk/auth|frazer.smith@ydh.nhs.uk\"",
		"jest": "jest",
		"jest:coverage": "jest --coverage",
		"lint": "eslint . --cache --ext js,jsx,ts,tsx --ignore-path .gitignore",
		"lint:licenses": "license-checker --direct --excludePackages=\"obfuscated-querystring@0.0.2\" --onlyAllow=\"0BSD;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;MIT;Python-2.0;Unlicense\" --production --summary",
		"lint:prettier": "prettier . -w -u",
		"lint:prettier:ci": "prettier . -c -u",
		"prepare": "husky install",
		"start": "node .",
		"start:dev": "nodemon | pino-pretty",
		"test": "npm run lint && npm run jest"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"jest": {
		"clearMocks": true,
		"collectCoverageFrom": [
			"src/**/*.js",
			"!src/app.js"
		],
		"coverageReporters": [
			"text",
			"lcovonly"
		],
		"resetMocks": true,
		"testEnvironment": "node",
		"testTimeout": 10000
	},
	"nodemonConfig": {
		"env": {
			"NODE_ENV": "development",
			"SUPPRESS_SUPPORT": 1
		},
		"ignore": [
			"test_resources"
		],
		"verbose": true,
		"watch": [
			".env",
			"src"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@faker-js/faker": "^7.5.0",
		"autocannon": "^7.10.0",
		"eslint": "^8.25.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^27.1.2",
		"eslint-plugin-jsdoc": "^39.3.6",
		"eslint-plugin-promise": "^6.1.0",
		"eslint-plugin-security": "^1.5.0",
		"eslint-plugin-security-node": "^1.1.1",
		"glob": "^8.0.3",
		"husky": "^8.0.1",
		"jest": "^29.2.0",
		"license-checker": "^25.0.1",
		"nock": "^13.2.9",
		"nodemon": "^2.0.20",
		"pino-pretty": "^9.1.1",
		"prettier": "^2.7.1"
	},
	"dependencies": {
		"@fastify/accepts": "^4.0.1",
		"@fastify/autoload": "^5.4.1",
		"@fastify/compress": "^6.1.1",
		"@fastify/cors": "^8.1.0",
		"@fastify/helmet": "^10.0.2",
		"@fastify/rate-limit": "^7.4.0",
		"@fastify/sensible": "^5.1.1",
		"@fastify/swagger": "^7.6.1",
		"@fastify/under-pressure": "^8.1.0",
		"ajv-formats": "^2.1.1",
		"axios": "^0.27.2",
		"dotenv": "^16.0.3",
		"env-schema": "^5.1.0",
		"fast-querystring": "^1.0.0",
		"fastify": "^4.9.1",
		"fastify-disablecache": "^3.0.3",
		"fastify-floc-off": "^2.0.3",
		"fastify-plugin": "^4.3.0",
		"file-stream-rotator": "^0.6.1",
		"fluent-json-schema": "^4.0.0",
		"js2xmlparser": "^5.0.0",
		"obfuscated-querystring": "git+https://git@github.com/BlackPearSw/obfuscated-querystring.git",
		"pino": "^8.6.1",
		"secure-json-parse": "^2.5.0",
		"upath": "^2.0.1"
	}
}
